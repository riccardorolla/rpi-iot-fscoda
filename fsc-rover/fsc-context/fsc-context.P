% facts
rover_request(IdChat,Cmd) :- rover_request_(IdChat,Cmd).
rover_command(Cmd,Out) :- rover_command_(Cmd,Out).
rover_validate(Out,Status) :- rover_validate_(Out,Status).
rover_next(Out,Cmd) :- rover_next_(Out,Cmd).
stop(Status) :- rover_command('get distance','0'),  
                 rover_validate('0',Status).

 



