% facts
request(IdChat,Cmd) :- request_(IdChat,Cmd).
execute(Cmd,Out) :- execute_(Cmd,Out).
observe(Obj,Status) :- observe_(Obj,Status).
rule(Obj,Status,Cmd) :- rule_(Obj,Status,Cmd).


response(IdChat,Out) :- request(IdChat,Cmd),execute(Cmd,Out).
next(Obj,Cmd) :- observe(Obj,Status),rule(Obj,Status,Cmd).
 
 



